{
  "name": "Quikotes",
  "description": "Take notes on the fly!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "worker.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": { "16": "logo.png", "48": "logo.png", "128": "logo.png" }
}
